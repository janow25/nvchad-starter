{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "objectscript",
      "camelcase": "Objectscript",
      "title": "Objectscript",
      "scope": "source.objectscript",
      "file-types": [
        ".mac",
        ".cls",
        ".inc"
      ],
      "injection-regex": "^objectscript$",
      "class-name": "TreeSitterObjectscript"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Objectscript grammar for tree-sitter",
    "authors": [
      {
        "name": "Janne Nowak"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-objectscript"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}